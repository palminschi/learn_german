    <!-- PHRASE + TRANSLATE -->
    <string name="phrase_translate_61">Он <b>врач</b> по профессии.</string>
    <string name="phrase_translate_62">В настоящее время я прохожу <b>обучение</b>.</string>
    <string name="phrase_translate_63">Я путешествую по <b>миру</b>.</string>
    <string name="phrase_translate_64">Он знает здесь много <b>людей</b>.</string>
    <string name="phrase_translate_65">Что делают твои <b>родители</b>?</string>
    <string name="phrase_translate_66">Я <b>живу</b> в квартире.</string>
    <string name="phrase_translate_67">никогда в <b>жизни</b></string>
    <string name="phrase_translate_68">Я <b>уже</b> здесь.</string>
    <string name="phrase_translate_69">Это <b>красиво</b>!</string>
    <string name="phrase_translate_70">Я люблю <b>тебя</b>!</string>
    <string name="phrase_translate_71"><b>Что</b> ты делаешь?</string>
    <string name="phrase_translate_72">Ты ездишь <b>быстро</b>.</string>
    <string name="phrase_translate_73">Я <b>женат</b>.</string>
    <string name="phrase_translate_74">Я <b>должен</b> работать.</string>
    <string name="phrase_translate_75">Я живу в <b>квартире</b>.</string>
    <string name="phrase_translate_76">Моя квартира имеет две <b>комнаты</b>.</string>
    <string name="phrase_translate_77">идти спать <i>(идти в кровать)</i></string>
    <string name="phrase_translate_78">Ей нравится <b>цветы</b>.</string>
    <string name="phrase_translate_79"><b>Это дерево</b> очень старое.</string>
    <string name="phrase_translate_80">Моя машина <b>зелёная</b>.</string>
